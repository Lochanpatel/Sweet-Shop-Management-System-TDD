{
  "name": "sweet-shop-backend",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "npx prisma migrate dev --name init",
